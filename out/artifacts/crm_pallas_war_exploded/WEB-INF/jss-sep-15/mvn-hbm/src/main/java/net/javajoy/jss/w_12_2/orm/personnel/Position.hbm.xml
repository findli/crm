<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.javajoy.jss.w_12_2.orm.personnel.Position" table="positions" schema="" catalog="personnel">
        <id name="id" column="id"/>
        <property name="name" column="name"/>
        <property name="comment" column="comment"/>
        <set name="employees" inverse="true">
            <key>
                <column name="fkPosition" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="net.javajoy.jss.w_12_2.orm.personnel.Employee"/>
        </set>
    </class>
</hibernate-mapping>